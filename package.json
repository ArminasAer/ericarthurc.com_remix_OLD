{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "run-s build:*",
    "build:css": "sass --no-source-map app/:app/",
    "build:remix": "remix build",
    "dev": "remix build && run-p dev:*",
    "dev:css": "sass --no-source-map --watch app/:app/",
    "dev:remix": "remix watch",
    "dev:node": "cross-env NODE_ENV=development nodemon --require dotenv/config ./server.ts --watch ./server.ts",
    "start": "cross-env NODE_ENV=production ts-node --require dotenv/config ./server.ts",
    "typecheck": "tsc -b"
  },
  "dependencies": {
    "@prisma/client": "^4.8.1",
    "@remix-run/express": "^1.10.1",
    "@remix-run/node": "^1.10.1",
    "@remix-run/react": "^1.10.1",
    "@remix-run/serve": "^1.10.1",
    "@types/redis": "^4.0.11",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "highlight.js": "^11.7.0",
    "isbot": "^3.6.5",
    "marked": "^4.2.5",
    "morgan": "^1.10.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "redis": "^4.5.1",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.10.1",
    "@remix-run/eslint-config": "^1.10.1",
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.15",
    "@types/marked": "^4.0.8",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.10",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.31.0",
    "nodemon": "^2.0.20",
    "npm-run-all": "^4.1.5",
    "prisma": "^4.8.1",
    "sass": "^1.57.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
